{{!-- components/c010-events.hbs --}}

<!-- COMPONENT START -->
{{#> sublayouts/grid-row }}
    {{#> sublayouts/grid-wide__col12 }}
        <div class="row">
            <div class="col-12">
                <div class="line time-line">
                    <span class="dot dot-start">&nbsp;</span>
                    <span class="dot dot-nov">&nbsp;</span>
                    <span class="dot dot-dec">&nbsp;</span>
                    <span class="dot dot-jan">&nbsp;</span>
                    <span class="dot dot-feb">&nbsp;</span>
                    <span class="dot dot-mar">&nbsp;</span>
                    <span class="dot dot-apr">&nbsp;</span>
                    <span class="dot dot-may">&nbsp;</span>
                    <span class="dot dot-end">&nbsp;</span>
                </div>
                <div class="line events-line">
                    <span class="line-label">Events</span>
                    <span class="dot dot-run dot-herts" data-toggle="tooltip" data-placement="top" title="Herts - 10km">&nbsp;</span>
                    <span class="dot dot-cycle dot-cycle1" data-toggle="tooltip" data-placement="top" title="Sportive - 48km ">&nbsp;</span>
                    <span class="dot dot-run dot-morun" data-toggle="tooltip" data-placement="top" title="Morun - 10km">&nbsp;</span>
                    <span class="dot dot-run dot-hatfield5" data-toggle="tooltip" data-placement="top" title="Hatfield 5 - 8km">&nbsp;</span>
                    <span class="dot dot-run dot-mkhalf" data-toggle="tooltip" data-placement="top" title="Milton Keynes Half">&nbsp;</span>
                    <span class="dot dot-dual dot-ashridgedual" data-toggle="tooltip" data-placement="top" title="Ashridge Duathlon">&nbsp;</span>
                    <span class="dot dot-run dot-windsorhalf" data-toggle="tooltip" data-placement="top" title="Windsor Half - 21km">&nbsp;</span>
                    <span class="dot dot-dual dot-ashridgedual2" data-toggle="tooltip" data-placement="top" title="Ashridge Duathlon again">&nbsp;</span>
                    <span class="dot dot-run dot-watfordhalf" data-toggle="tooltip" data-placement="top" title="Watford Half">&nbsp;</span>

                    <!--

                    <span class="dot dot-tri dot-marlowe dot-to-book" data-toggle="tooltip" data-placement="top" title="Marlowe - Olympic"></span>
                    <span class="dot dot-cycle dot-cycle2 dot-to-book" data-toggle="tooltip" data-placement="top" title="Sportive 2 ???"></span>
                    <span class="dot dot-run dot-harpenden dot-to-book" data-toggle="tooltip" data-placement="top" title="Harpenden Half - 21km"></span>
                    <span class="dot dot-cycle dot-cycle3 dot-to-book" data-toggle="tooltip" data-placement="top" title="Sportive 3 ???"></span>
                    <span class="dot dot-cycle dot-cycle4 dot-to-book" data-toggle="tooltip" data-placement="top" title="Sportive 4 ???"></span>
                    -->
                </div>
                <div class="line target-line">
                    <span class="line-label">Targets</span>
                    <span class="dot dot-target dot-target-herts" data-toggle="tooltip" data-placement="top" title="00:44:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-cycle1" data-toggle="tooltip" data-placement="top" title="02:09:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-morun" data-toggle="tooltip" data-placement="top" title="00:43:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-hatfield5" data-toggle="tooltip" data-placement="top" title="00:34:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-mkhalf" data-toggle="tooltip" data-placement="top" title="01:49:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-ashridge" data-toggle="tooltip" data-placement="top" title="02:59:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-windsorhalf" data-toggle="tooltip" data-placement="top" title="01:39:59">&nbsp;</span>
                    <span class="dot dot-target dot-target-ashridge2" data-toggle="tooltip" data-placement="top" title="02:49:59">&nbsp;</span>
                    <span class="dot dot-target dot-watfordhalf" data-toggle="tooltip" data-placement="top" title="01:39:59">&nbsp;</span>
                </div>
                <div class="line current-line">
                    <span class="line-label">Status</span>
                    <span class="dot dot-target-herts dot-success" data-toggle="tooltip" data-placement="top" title="00:43:40">&nbsp;</span>
                    <span class="dot dot-target-cycle1 dot-success" data-toggle="tooltip" data-placement="top" title="02:04:00">&nbsp;</span>
                    <span class="dot dot-target-morun dot-success" data-toggle="tooltip" data-placement="top" title="00:43:10">&nbsp;</span>
                    <span class="dot dot-target-hatfield5 dot-success" data-toggle="tooltip" data-placement="top" title="00:34:26">&nbsp;</span>
                    <span class="dot dot-target-mkhalf dot-success" data-toggle="tooltip" data-placement="top" title="01:39:23">&nbsp;</span>
                    <span class="dot dot-target-ashridge dot-success" data-toggle="tooltip" data-placement="top" title="02:48:23">&nbsp;</span>
                    <span class="dot dot-target-windsorhalf dot-fail" data-toggle="tooltip" data-placement="top" title="Didn't enter">&nbsp;</span>
                    <span class="dot dot-target-ashridge2 dot-success" data-toggle="tooltip" data-placement="top" title="02:43:25">&nbsp;</span>
                </div>
            </div>
        </div>
    {{/sublayouts/grid-wide__col12 }}
{{/sublayouts/grid-row }}
<!-- COMPONENT END -->
